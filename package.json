{
  "name": "dreadmap",
  "version": "1.0.0",
  "description": "An interactive map for Metroid Dread",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run compile:js && npm run start:ws",
    "debug": "npm run watch:js & npm run debug:ws",
    "compile:js": "browserify client/render_map.cjs --s module -o public/bundle.js",
    "watch:js": "watchify client/render_map.cjs --s module -o public/bundle.js",
    "start:ws": "node app.js",
    "debug:ws": "nodemon app.js"
  },
  "keywords": [
    "dread"
  ],
  "author": "Michael James Ignatoski",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.1",
    "leaflet": "^1.7.1",
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "watchify": "^4.0.0"
  }
}
